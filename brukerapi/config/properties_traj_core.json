{
  "numpy_dtype": [
    {
      "cmd": "np.dtype('float64').newbyteorder('<')",
      "conditions": []
    }
  ],
  "scheme_id": [
    {
      "cmd": "'RADIAL'" ,
      "conditions": [
        ["#PULPROG[1:-1]",
          [
            "UTE.ppg",
            "UTE3D.ppg",
            "ZTE.ppg",
            "mcw_GARadialv1.ppg"
          ]
        ],
        "#ACQ_dim==2",
        "#ACQ_sw_version in ['<PV 5.1>', '<PV 6.0>', '<PV 6.0.1>', '<PV-7.0.0>']"
      ]
    },     
    {
      "cmd": "'RADIAL_3D'" ,
      "conditions": [
        ["#PULPROG[1:-1]",
          [
            "mcw_GARadialv1.ppg"
          ]
        ],
        "#ACQ_dim==3",
        "#ACQ_sw_version in ['<PV 5.1>', '<PV 6.0>', '<PV 6.0.1>', '<PV-7.0.0>']"
      ]
    }, 
    {
      "cmd": "'SPIRAL'" ,
      "conditions": [
        ["#PULPROG[1:-1]",
          [
            "SPIRAL.ppg",
            "DtiSpiral.ppg"
          ]
        ],
        ["#ACQ_sw_version",
          [
            "<PV 6.0.1>",
            "<PV 5.1>"
          ]
        ]
      ]
    },
    {
      "cmd": "'ZTE'" ,
      "conditions": [
        ["#PULPROG[1:-1]",
          [
            "ZTE.ppg"
          ]
        ],
        ["#ACQ_sw_version",
          [
            "<PV 6.0.1>",
            "<PV 5.1>"
          ]
        ]
      ]
    }
  ],
  "traj_type": [
    {
      "cmd": "'radial'",
      "conditions": [["@scheme_id",["RADIAL","ZTE","RADIAL_3D"]]]
    },
    {
      "cmd": "'spiral'",
      "conditions": [["@scheme_id",["SPIRAL"]]]
    }
  ],
  "shape_storage": [
    {
      "cmd": "(#PVM_TrajDims,#PVM_Matrix[0],#NPro)",
      "conditions": [["@scheme_id",["RADIAL","ZTE","RADIAL_3D"]]]
    },
    {
      "cmd": "(#ACQ_dim,#PVM_TrajSamples,#PVM_SpiralNbOfInterleaves)",
      "conditions": [["@scheme_id",["SPIRAL"]]]
    }
  ],
  "permute": [
    {
      "cmd": [
          0,
          1,
          2
        ],
      "conditions": [["@scheme_id",["RADIAL","RADIAL_3D","ZTE","SPIRAL"]]]
    }
  ],
  "final": [
    {
      "cmd": [
        "#PVM_TrajDims",
        "#PVM_Matrix[0]",
        "#NPro"
      ],
      "conditions": [["@scheme_id",["RADIAL","RADIAL_3D","ZTE"]]]
    },
    {
      "cmd": [
        "#ACQ_dim",
        "#ACQ_size[0]//2",
        "#PVM_SpiralNbOfInterleaves"
      ],
      "conditions": [["@scheme_id",["SPIRAL"]]]
    }
  ]
}